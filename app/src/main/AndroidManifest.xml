<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
		  package="com.manitowocfoodservice.lincolnovencapacitycalculator">
	<uses-permission android:name="android.permission.INTERNET"/>
	<application
			android:allowBackup="true"
			android:icon="@mipmap/app_icon"
			android:label="@string/app_name"
			android:logo="@mipmap/app_logo"
			android:theme="@style/AppTheme">
		<activity
				android:name=".MainMenu"
				android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>
				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>
		<activity
				android:name=".CapacityCalculatorActivity"
				android:label="@string/title_activity_capacity_calculator"
				android:parentActivityName=".MainMenu">
			<meta-data
					android:name="android.support.PARENT_ACTIVITY"
					android:value="com.manitowocfoodservice.lincolnovencapacitycalculator.MainMenu"/>
		</activity>
		<activity
				android:name=".SpecSheetLinks"
				android:label="@string/title_activity_spec_sheet_links"
				android:parentActivityName=".MainMenu">
			<meta-data
					android:name="android.support.PARENT_ACTIVITY"
					android:value="com.manitowocfoodservice.lincolnovencapacitycalculator.MainMenu"/>
		</activity>
		<activity
				android:name=".OvenComparison"
				android:label="@string/title_activity_oven_comparison"
				android:parentActivityName=".MainMenu">
			<meta-data
					android:name="android.support.PARENT_ACTIVITY"
					android:value="com.manitowocfoodservice.lincolnovencapacitycalculator.MainMenu"/>
		</activity>
		<activity
				android:name=".SpecSheet"
				android:label="@string/title_activity_spec_sheet"
				android:parentActivityName=".SpecSheetLinks">
			<meta-data
					android:name="android.support.PARENT_ACTIVITY"
					android:value="com.manitowocfoodservice.lincolnovencapacitycalculator.SpecSheetLinks"/>
		</activity>
	</application>

</manifest>
